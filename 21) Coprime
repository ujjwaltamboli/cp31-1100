#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }
        vector<int> ind(1001,0);

        for (int i=0;i<n;i++){
            ind[store[i]]=i+1;
        }
        int maxi=0;
        for (int i=1;i<=1000;i++){//1000
            if (ind[i]==0){
                continue;
            }
            for (int j=1;j<=1000;j++){//1000
                if (ind[j]==0){
                    continue;
                }
                if (__gcd(i,j)==1){//O(log(min(a,b)))
                    maxi=max(maxi,ind[i]+ind[j]);
                }
            }
        }
        if (maxi==0){
            cout<<-1<<endl;
            continue;
        }
        cout<<maxi<<endl;
        //T.C.:- n2log(1000);
        //S.C.:- O(n+1000);

    }
    return 0;
}
