#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        string s;
        cin >> s;
        int n = s.length();
        set<char> st;
        for (int i = 0; i < n; i++)
        {
            st.insert(s[i]);
        }
        int d = st.size();
        int i = 0;
        bool flag = true;
        while (i + d < n)
        {
            if (s[i] != s[i + d])
            {
                flag = false;
                break;
            }
            i++;
        }

        if (!flag)
        {
            cout << "No" << endl;
        }
        else
        {
            cout << "yes" << endl;
        }

        //T.C.:- O(n);
        //S.C.:- O(n);
    }
}
