#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n,q;
        cin>>n>>q;
        vector<int> a(n,0);
        vector<int> x(q,0);
        for (int i=0;i<n;i++){
            cin>>a[i];
        }
        for (int i=0;i<q;i++){
            cin>>x[i];
        }

        int sum=0;
        int mini=INT_MAX;
        for (int i=0;i<q;i++){
            if (x[i]<mini){
                sum=sum+pow(2,x[i]-1);
                mini=x[i];
            }
        }

        for (int i=0;i<n;i++){
            int rightmost_on_bit=a[i]&(-a[i]);
            int corrected_sum=sum&(rightmost_on_bit-1);
            cout<<a[i]+corrected_sum<<" ";
        }
        cout<<endl;

        //T.C.:- O(n+q)
        //S.C.:- O(n+q)
    }
    return 0;
}
