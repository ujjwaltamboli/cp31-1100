#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        string s;
        cin>>s;
        vector<bool> present(26,false); 
        int count=0;
        for (int i=0;i<n;i++){
            int ch=s[i]-'a';
            if (!present[ch]){
                count=count+n-i;
                present[ch]=true;
            }
        }
        cout<<count<<endl;

        //T.C.:- O(n);
        //S.C.:- O(n);
    }
    return 0;
}
