#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        long long n;
        cin>>n;
        vector<vector<long long>> store(n,vector<long long>(2,0));
        for (long long i=0;i<n;i++){
            cin>>store[i][0];
            store[i][1]=i;
        }
        sort(store.rbegin(), store.rend());
        long long sum=0;
        vector<long long> prefixSum(n,0);
        for (long long i=n-1;i>=0;i--){
            sum=sum+store[i][0];
            prefixSum[i]=sum;
        }

        vector<long long> canTake(n,0);
        canTake[0]=0;
        for (long long i=1;i<n;i++){
            if (prefixSum[i]>=store[i-1][0]){
                canTake[i]=1+canTake[i-1];
            }
        }
        vector<long long> ans(n,0);
        for (long long i=0;i<n;i++){
            ans[i]=canTake[i]+n-i-1;
        }

        vector<long long> ans2(n,0);
        for (long long i=0;i<n;i++){
            ans2[store[i][1]]=ans[i];
        }

        for (long long i=0;i<n;i++){
            cout<<ans2[i]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
