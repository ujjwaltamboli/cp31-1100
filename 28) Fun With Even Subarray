#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        long long n;
        cin>>n;
        vector<long long> store(n,0);
        for (long long i=0;i<n;i++){
            cin>>store[i];
        }

        reverse(store.begin(), store.end());

        long long val=store[0];
        long long op=0;
        for (long long i=0;i<n;){
            if (store[i]!=val){
                i=i*2;
                op++;
            }
            else{
                i++;
            }
        }
        cout<<op<<endl;

        //T.C.:- O(n);
        //S.C.:- O(n);
    }
    return 0;
}
