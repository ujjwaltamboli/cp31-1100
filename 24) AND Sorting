#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){

        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }
        int sum=INT_MAX;
        for (int i=0;i<n;i++){
            if (i!=store[i]){
                sum=sum&store[i];
            }
        }
    
        cout<<sum<<endl;

        //T.C.:- O(n);
        //S.C.:- O(n);
    }
}
